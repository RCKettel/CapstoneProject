{
   "$schema": "http://json-schema.org/schema#",
   "type": "array",
   "items": {
      "type": "object",
      "properties": {
         "mediaartdb": {
            "type": "object",
            "properties": {
               "id": {
                  "type": "string"
               },
               "title": {
                  "type": "string"
               },
               "alt_titles": {
                  "type": "array",
                  "items": {
                     "type": "string"
                  }
               },
               "platform": {
                  "type": "string"
               },
               "year": {
                  "type": [
                     "integer",
                     "null"
                  ]
               }
            },
            "required": [
               "alt_titles",
               "id",
               "platform",
               "title",
               "year"
            ]
         },
         "mobygames": {
            "type": "array",
            "items": {
               "type": "object",
               "properties": {
                  "id": {
                     "type": "string"
                  },
                  "title": {
                     "type": "string"
                  },
                  "alt_titles": {
                     "type": "array",
                     "items": {
                        "type": "string"
                     }
                  },
                  "platforms": {
                     "type": "array",
                     "items": {
                        "type": [
                           "null",
                           "string"
                        ]
                     }
                  },
                  "years": {
                     "type": "array",
                     "items": {
                        "type": "integer"
                     }
                  },
                  "slug": {
                     "type": "string"
                  },
                  "link_value": {
                     "type": "number"
                  }
               },
               "required": [
                  "alt_titles",
                  "id",
                  "link_value",
                  "platforms",
                  "slug",
                  "title",
                  "years"
               ]
            }
         }
      },
      "required": [
         "mediaartdb",
         "mobygames"
      ]
   }
}
